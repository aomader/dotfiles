altscreen on
bell_msg '%c bell -> %n%f %t^G'
hardstatus string "%h%t"
caption always '%{= dw} %-w%{= KW} %n %t %{= dw}%+w %L>%= %{= dK}%D %d %c %{= dd}'
msgwait 2
startup_message off
term screen-256color
termcapinfo rxvt* 'hs:ts=\E]2;:fs=\007:ds=\E]2;screen (not title yet)\007'
vbell off

defscrollback 5000
defutf8 on
